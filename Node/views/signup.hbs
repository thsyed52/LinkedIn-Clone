
<form method="post" action="/users/signup">
<div class="box">
<h1>Dashboard</h1>

<input type="email" name="email" value="email" onFocus="field_focus(this, 'email');" onblur="field_blur(this, 'email');" class="email" />
  
<input type="password" name="password" value="" onFocus="field_focus(this, 'email');" onblur="field_blur(this, 'email');" class="password" />
  
<button type="submit">Sign up</button>  

</div> <!-- End Box -->
  
</form>
